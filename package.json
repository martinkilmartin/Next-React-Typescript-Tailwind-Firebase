{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "imagemin": "node --trace-warnings ./scripts/gm-resize.mjs && node --trace-warnings ./scripts/imagemin-webp.mjs && node --trace-warnings ./scripts/imagemin-mozjpeg.mjs",
    "dev": "tsc && next",
    "build": "next build",
    "start": "next start",
    "build-export": "next build && next export",
    "deploy": "firebase deploy --only hosting",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "daisyui": "^1.2.0",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "theme-change": "^2.0.2"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "autoprefixer": "^10.2.5",
    "gm": "^1.23.1",
    "husky": ">=6",
    "imagemin": "^8.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.2.10",
    "prettier": "2.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "4.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write"
    ]
  }
}
