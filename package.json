{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsc && next",
    "build": "next build",
    "start": "next start",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "autoprefixer": "^10.2.5",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.2.10",
    "prettier": "2.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "4.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write"
    ]
  }
}
